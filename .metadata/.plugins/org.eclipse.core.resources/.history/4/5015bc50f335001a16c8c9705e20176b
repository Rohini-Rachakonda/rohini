import java.io.*;
public class Ioperations {

	public static void main(String args[]){
		try{
			FileOutputStream fout=new FileOutputStream("E:\\IOStream.txt");
			String s="Welcome to My Java Notes.";
			byte b[]=s.getBytes();
			fout.wait(7);
			fout.close();
			System.out.println("Sucess");
		}catch(Exception e)
		{
			System.out.println(e);
		}
	}
}
